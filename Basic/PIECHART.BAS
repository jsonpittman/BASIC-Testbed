38 LOCATE38,23:FL=0
40 A$=INKEY$:IFA$="S" THEN FL=1:IF SH=1 THEN SH=0:GOTO32 ELSE SH=1:GOTO32
42 IFA$="T" THEN LOCATE9,22:ATTR6,0:PRINT STRING$(60,32);:LOCATE9,22:LINEINPUT T$:T=LEN(T$):T$=LEFT$(T$,28):IF T>28 THEN 20 ELSE 36
44 IF A$<>"M" THEN 50 ELSE LOCATE0,22:PRINTSTRING$(40,32);:LOCATE0,22:LINEINPUT"Modify what unit (1-14/999 = exit) >";M$:M=VAL(M$):IF M=0 OR M>14 THEN 50
