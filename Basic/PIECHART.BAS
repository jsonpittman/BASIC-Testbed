96 CO=CO+1:IF CO=MC+1 THEN CO=ST
98 NEXTY:HCIRCLE(X1,Y1),RD,0:HCOLOR15:A$=STRING$((40-LEN(T$))/2-1,59)+" "+T$+" "+STRING$((40-LEN(T$))/2-1,59):HPRINT(0,0),A$:RETURN
100 A=AN/57.141*3.6:X2=X1+SIN(A)*RD:Y2=Y1-COS(A)*RD:RETURN
