22 PRINT"CONTROL SETTINGS";
24 ATTR0,0:T=0:FORY=1TO14:T=T+D(Y):NEXTY:IF T=0 THEN T=.01
26 FORY=1 TO 14:LOCATE0,6+Y:PRINTSTRING$(26,32);:LOCATE3-LEN(STR$(Y)),6+Y:PRINTY;:LOCATE3,6+Y:PRINT")";:LOCATE4,6+Y:PRINTD(Y);:LOCATE10,6+Y:PRINTD$(Y);:Z=INT((D(Y)/T)*100)
28 Z$=STR$(Z):Z$=RIGHT$(Z$,LEN(Z$)-1)+"%":LOCATE 25-LEN(Z$),6+Y:PRINTZ$;:NEXTY:ATTR5,0:LOCATE26,8:PRINT"(S)hadow Pie?";
