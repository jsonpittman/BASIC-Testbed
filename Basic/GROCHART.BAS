10 'HEIGHT CHART PRINTER FOR GEMINI 10X
20 POKE 150,18
30 PRINT#-2,CHR$(27);CHR$(65);CHR$(18);' 1/4 INCH LINEFEEDS
40 CLS
50 LINEINPUT"ENTER NAME OF CHILD: ";CN$
60 PRINT#-2,"Height Growth Chart for ";CN$
70 PRINT#-2:PRINT#-2
80 F=5:I=0
90 PRINT#-2,F" Feet  "I "inches";TAB(22);"----"
100 PRINT#-2,TAB(22);"-"
110 PRINT#-2,TAB(22);"--"
120 PRINT#-2,TAB(22);"-"
130 I=I-1:IFI<0 THENI=11:F=F-1
140 IFF>1 THEN 90
150 END
