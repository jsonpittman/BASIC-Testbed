0 CLS:CLEAR1000:CLEAR200,&H3EFF
1 PRINT" *** EDITOR ***"
2 FORT=1TO1000:NEXTT
3 INPUT"LOAD OLD FILE(Y/N):";C$
4 IFC$="Y" THEN GOSUB11:CLOADM"ML SONG":GOSUB14:ELSE 5
5 CLS:INPUT"START ADDRESS:";A
6 FORX=A TO &H4F24:PRINTX;:INPUT":&H";A$
7 IF A$="S" THEN 10
8 POKEX,VAL(HEX$(A$))
9 NEXTX
10 GOSUB11:CSAVEM"ML SONG",&H3F00,&H4F24,1:END
11 PRINT"READY CASSETTE(PRESS ANY KEY)"
12 IF INKEY$=""THEN12
13 RETURN
14 INPUT"MAKE PRINTOUT";C$:IFC$="Y" THEN 15 ELSE RETURN
15 FORT=&H3F00 TO &H4F24 STEP12:FOR TT=0 TO 11:PRINT#-2,HEX$(PEEK(T+TT));"  ";:NEXTTT:PRINT#-2:NEXTT
