10 CLS
20 PRINT" THIS PROGRAM SOLVES QUADRATIC   EQUATIONS USING THE QUADRATIC   FORMULA."
25 PRINT
30 INPUT" COEFFICIENT A:";A
40 INPUT" COEFFICIENT B:";B
50 INPUT" COEFFICIENT C:";C
60 J=4*A*C
70 K=B*B
80 L=K-J
81 IF L<0 THEN PRINT"  THERE ARE NO REAL SOLUTIONS     TO THIS QUADRATIC EQUATION.":GOTO 190
90 M=SQR(L)
100 N=-B+M
110 O=2*A
120 P=N/O
130 Q=-B-M
140 R=Q/O
150 PRINT
160 PRINT" 1ST SOLUTION:";P
170 PRINT" 2ND SOLUTION:";R
180 PRINT
190 INPUT"DO YOU WISH TO SOLVE ANOTHER";A$
200 IF A$="Y" THEN10 ELSE IFA$="N" THEN END
