10 REM  *************
20 REM  *  VIDTIME  *
30 REM  *************
40 A = .0188418559 : B = 1.01131207
50 PRINT"INPUT TAPE SPEED" : PRINT"   F FOR FAST" : PRINT"   M FOR MEDIUM" : PRINT"   S FOR SLOW"   
60 INPUT Q$
70 IF Q$="F" THEN D=1 : GOTO 110
80 IF Q$="M" THEN D=2 : GOTO 110
90 IF Q$="S" THEN D=3 : GOTO 110
100 END
110 INPUT"COUNT 1 = ";X1 : X1=X1/100
120 INPUT"COUNT 2 = ";X2 : X2=X2/100
130 Y=D*(A*(X2*X2-X1*X1)+B*(X2-X1))
140 PRINT"TIME INTERVAL = ";INT(Y);" MINUTES" : GOTO 50
