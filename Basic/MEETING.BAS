100 ' THE MEETING
110 DIM M(224)
120 FOR W=1 TO 7
130 WD=INT(2^(7-W+5))
140 FOR J=0 TO 31
150 M((W-1)*32+J+1)=WD+J
160 NEXT J: NEXT W
170 FOR I=1 TO 224: IF (M(I) AND 5)=5 THEN M(I)=0
180 NEXT
190 FOR I=1 TO 224: IF (M(I) AND 24)=24 THEN M(I)=0
200 NEXT
210 FOR I=1 TO 224: IF (M(I) AND 16)=16 THEN IF (M(I) AND 4) <> 4 THEN M(I)=0
220 NEXT
230 FOR I=1 TO 224: IF (M(I) AND 16)<>16 THEN M(I)=0
240 NEXT
250 FOR I=1 TO 224
260 CT=0
270 IF (M(I) AND 16)=16 THEN CT=CT+1
280 IF (M(I) AND 8)=8 THEN CT=CT+1
290 IF (M(I) AND 4)=4 THEN CT=CT+1
300 IF (M(I) AND 2)=2 THEN CT=CT+1
310 IF (M(I) AND 1)=1 THEN CT=CT+1
320 IF CT<3 THEN M(I)=0
330 NEXT
340 FOR I=1 TO 224: IF (M(I) AND 2)=2 THEN IF (M(I) AND 2048) <> 2048 THEN M(I)=0
350 NEXT
360 FOR I=1 TO 224: IF M(I)<>0 THEN PRINT M(I);
370 NEXT
