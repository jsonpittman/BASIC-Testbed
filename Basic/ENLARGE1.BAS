5 'RAINBOW MAGAZINE
10 S=3:S4=S*S*S*S:S3=S*S*S:S2=S*S
20 SS=S-1
30 DIM S(SS,SS)
40 FOR X=0 TO SS:FOR Y=0 TO SS
50 S(X,Y)=1:NEXT Y,X:S(2,1)=0:S(1,2)=0:S(2,2)=0
60 'S(X,Y)=1:NEXT Y,X:S(1,1)=0
70 PMODE4,1:COLOR0,5:PCLS:SCREEN1,1
80 FOR X4=0 TO SS
90 FOR Y4=0 TO SS
100 IF S(X4,Y4)=1 THEN GOSUB 130
110 NEXT Y4,X4
120 GOTO 120
130 FOR X3=0 TO SS
140 FOR Y3=0 TO SS
150 IF S(X3,Y3)=1 THEN GOSUB 170
160 NEXT Y3,X3:RETURN
170 FOR X2=0 TO SS
180 FOR Y2=0 TO SS
190 IF S(X2,Y2)=1 THEN GOSUB 210
200 NEXT Y2,X2:RETURN
210 FOR X1=0 TO SS
220 FOR Y1=0 TO SS
230 IF S(X1,Y1)=1 THEN GOSUB 250
240 NEXT Y1,X1:RETURN
250 FOR X0=0 TO SS
260 FOR Y0=0 TO SS
270 IF S(X0,Y0)=0 THEN 310
280 XX=S4*X4+S3*X3+S2*X2+S*X1+X0
290 YY=S4*Y4+S3*Y3+S2*Y2+S*Y1+Y0:IF YY>191 THEN 310
300 PSET(XX,YY)
310 NEXT Y0,X0:RETURN
